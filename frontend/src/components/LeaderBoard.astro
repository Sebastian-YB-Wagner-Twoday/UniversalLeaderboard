---
import type { Contest } from "../model/Contest.model";

interface Props {
  contest: Contest;
}

const { contest } = Astro.props;
---

<div>
  <p class="text-2xl">{contest.name}</p>
  {contest.description ? <p class="text-xl">{contest.description}</p> : ""}

  <p>scores</p>
</div>
